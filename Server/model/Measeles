    SName:String,
    Eschool:String,
    Hfacility:Date,
    visite:String,
    PType:String,
    Weight:String,
    Hight:String,
    BMI:String,
    MUAC:String,
    PPhone:String,
    HRegion:String,
    HTRegion:String,
    HSubCity:String,
    THSubCity:String,
    HWoreda:String,
    HFTworeda:String,
    HFName:String,
    HFTName:String,
    NotifiedDate:Date,
    OnsetDate:Date,
    Orash:Date,
    MNumber:String,
    Vdate:Date,
    Aepidemics:String,
    visited:String,
    Treatment:String,
    TTreatment:String,
    visiblee:String,
